  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.18.2",
    "helmet": "^6.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
